{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "statics/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/playlist",
      "dest": "statics/playlist.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "statics/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ]
}
